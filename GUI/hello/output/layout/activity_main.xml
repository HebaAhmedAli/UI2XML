<?xml version = "1.0" encoding = "utf-8"?>
<LinearLayout xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:app = "http://schemas.android.com/apk/res-auto"
	xmlns:tools = "http://schemas.android.com/tools"
	android:layout_width = "match_parent"
	android:layout_height = "match_parent"
	android:orientation = "vertical"
	tools:context = ".MainActivity">
	<LinearLayout
		android:orientation = "horizontal"
		android:background = "#ffffff"
		android:layout_width = "match_parent"
		android:layout_height = "0dp"
		android:layout_weight = "2"
		android:gravity = "center"
		>
		<ImageView
			android:id = "@+id/ImageView_0_1_0"
			android:padding="5dp"
			android:layout_height = "wrap_content"
			android:layout_width = "wrap_content"
			android:src = "@drawable/pic_190_136"
			>
		</ImageView>
	</LinearLayout>
	<LinearLayout
		android:orientation = "horizontal"
		android:background = "#ffffff"
		android:layout_width = "match_parent"
		android:layout_height = "0dp"
		android:layout_weight = "1"
		android:gravity = "center"
		>
		<EditText
			android:id = "@+id/EditText_0_2_0"
			android:padding="5dp"
			android:layout_height = "wrap_content"
			android:layout_width = "wrap_content"
			android:hint = "eee"
			android:ems = "29"
			>
		</EditText>
	</LinearLayout>
	<LinearLayout
		android:orientation = "horizontal"
		android:background = "#ffffff"
		android:layout_width = "match_parent"
		android:layout_height = "0dp"
		android:layout_weight = "2"
		android:gravity = "center"
		>
		<EditText
			android:id = "@+id/EditText_0_3_0"
			android:padding="5dp"
			android:layout_height = "wrap_content"
			android:layout_width = "wrap_content"
			android:hint = "Fasswoid"
			android:ems = "29"
			>
		</EditText>
	</LinearLayout>
	<LinearLayout
		android:orientation = "horizontal"
		android:background = "#ffffff"
		android:layout_width = "match_parent"
		android:layout_height = "0dp"
		android:layout_weight = "1"
		android:gravity = "center"
		>
		<Button
			android:id = "@+id/Button_0_4_0"
			android:padding="5dp"
			android:layout_height = "wrap_content"
			android:layout_width = "wrap_content"
			android:text = "LOG IN"
			android:textColor = "#f5ffff"
			android:background = "#041533"
			android:onClick = "clickMe0_4_0"
			android:gravity = "center"
			>
		</Button>
	</LinearLayout>
	<LinearLayout
		android:orientation = "horizontal"
		android:background = "#ffffff"
		android:layout_width = "match_parent"
		android:layout_height = "0dp"
		android:layout_weight = "2"
		android:gravity = "center"
		>
		<Button
			android:id = "@+id/Button_0_5_0"
			android:padding="5dp"
			android:layout_height = "wrap_content"
			android:layout_width = "wrap_content"
			android:text = "forget the password"
			android:textColor = "#4c7da6"
			android:background = "#ffffff"
			android:onClick = "clickMe0_5_0"
			android:gravity = "center"
			>
		</Button>
	</LinearLayout>
	<LinearLayout
		android:orientation = "horizontal"
		android:background = "#ffffff"
		android:layout_width = "match_parent"
		android:layout_height = "0dp"
		android:layout_weight = "1"
		android:gravity = "center"
		>
		<Button
			android:id = "@+id/Button_0_6_0"
			android:padding="5dp"
			android:layout_height = "wrap_content"
			android:layout_width = "wrap_content"
			android:text = "Don t have an account REGISTER"
			android:textColor = "#5d5d5d"
			android:background = "#ffffff"
			android:onClick = "clickMe0_6_0"
			android:gravity = "center"
			>
		</Button>
	</LinearLayout>
</LinearLayout>
